#use <conio>
#use <string>

int search(int x, int[] A, int n);

int main()
{
  int[] A = alloc_array(int, 10);
  A[7] = 3;
  int t = search(2,A,10);
  printint(t);println("");
  return 0;
}

int search(int x, int[] A, int n)
//@requires \length(A)==n;
//@ensures A[\result] == x;
{
  for (int i=0; i<n-2; i++){
    if (A[i] == x){
      return i;
    }
  }
  return -1;
}